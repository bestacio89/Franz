{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Database": {
    "ServerName": "localhost",
    "DatabaseName": "sample",
    "UserName": "root",
    "Password": "password",
    "Port": "3308",
    "SslMode": "Preferred"
  },
  "Messaging": {
    "HostName": "localhost",
    "UserName": "guest",
    "Password": "guest",
    "Port": "5672",
    "SslEnabled": false,
    "VirtualHost": "/"
  },
  "Resilience": {
    "RetryPolicy": {
      "Enabled": true,
      "RetryCount": 3,
      "RetryIntervalMilliseconds": 500
    },
    "CircuitBreaker": {
      "Enabled": true,
      "FailureThreshold": 0.5,
      "MinimumThroughput": 10,
      "DurationOfBreakSeconds": 30
    },
    "TimeoutPolicy": {
      "Enabled": true,
      "TimeoutSeconds": 5
    },
    "BulkheadPolicy": {
      "Enabled": true,
      "MaxParallelization": 10,
      "MaxQueueSize": 20
    }
  },
  "TIdType": "System.Int32"
}
